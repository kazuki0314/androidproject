        android:layout_marginEnd="54dp"
        android:layout_marginBottom="612dp"
        android:fontFamily="@font/rammetto_one"
        android:text="Support"
        android:textAlignment="center"
        android:textIsSelectable="false"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/spaddbt"
        style="@style/Widget.Material3.Button"
        android:layout_width="124dp"
        android:layout_height="37dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="578dp"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="116dp"
        android:backgroundTint="@color/white"
        android:text="Add"
        android:textColor="@color/black"
        app:icon="@android:drawable/ic_menu_add"
        app:iconTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editspname"
        android:layout_width="304dp"
        android:layout_height="39dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="543dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/spatt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="559dp"
        android:layout_marginEnd="284dp"
        android:layout_marginBottom="153dp"
        android:text="Attachement"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioButton
        android:id="@+id/spradio"
        android:layout_width="304dp"
        android:layout_height="26dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="630dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="75dp"
        android:text="Make sure your entry is correct"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editspEmailAddress"
        android:layout_width="305dp"
        android:layout_height="33dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="211dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="487dp"
        android:contentDescription="your email"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editsptext"
        android:layout_width="304dp"
        android:layout_height="40dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="401dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="290dp"
        android:ems="10"
        android:inputType="text"
        android:text="Subject"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editspPhone"
        android:layout_width="305dp"
        android:layout_height="33dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="270dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="428dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/sptel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="247dp"
        android:layout_marginEnd="191dp"
        android:layout_marginBottom="461dp"
        android:fontFamily="@font/eb_garamond"
        android:text="Phone number (Not Neccecary)"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spdrop1"
        android:layout_width="305dp"
        android:layout_height="42dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="359dp"
        android:spinnerMode="dropdown"
        android:splitMotionEvents="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/spcategorytext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="306dp"
        android:layout_marginEnd="265dp"
        android:layout_marginBottom="402dp"
        android:fontFamily="@font/eb_garamond"
        android:text="Problem Category"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/spsendbt"
        style="@style/Widget.Material3.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="149dp"
        android:layout_marginTop="656dp"
        android:layout_marginEnd="155dp"
        android:layout_marginBottom="27dp"
        android:backgroundTint="#F1F8E9"
